<header>
  <nav class="navbar navbar-expand-lg navbar-light bg-light">
    <a class="navbar-brand" href="/">
      <img src="/img/logo.png" width="40" height="40" class="d-inline-block align-top" alt="">DayIn
    </a>

    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
      aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNav">
      <ul class="navbar-nav ml-md-auto">
        {{#if user}}
        {{#if user.isAdmin}}
        <li class="nav-item active">
          <a class="nav-button nav-link" href="/admin-dashboard">Account</a>
        </li>
        {{else}}
        <li class="nav-item active">
          <a class="nav-button nav-link" href="/user-dashboard">Account</a>
        </li>
        {{/if}}
        <li class="nav-item active">
          <a class="nav-button nav-link" href="/log-out">Log Out</a>
        </li>
        {{else}}

        <li class="nav-item active">
          <a class="nav-button nav-link" href="/log-in">Log In</a>
        </li>
        <li class="nav-item active">
          <a class="nav-button nav-link" href="/sign-up">Sign Up</a>
        </li>
        {{/if}}
      </ul>
    </div>
  </nav>
</header>